    "code-runner.executorMap": {
        //     "cpp": "cd $dir && g++ -std=c++20 $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
        //     "c++": "cd $dir && g++ -std=c++20 $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
        "cpp": "cd $dir && clang++ --target=x86_64-w64-windows-gnu -std=c++20 -Wall -Wextra -g $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
        "c++": "cd $dir && clang++ --target=x86_64-w64-windows-gnu -std=c++20 $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
    },
    "github.copilot.enable": {
        "*": false,
        "plaintext": false,
        "markdown": false,
        "scminput": false
